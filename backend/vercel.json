{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "index.js"
    },
    {
      "src": "/api/v1/auth/(.*)",
      "dest": "index.js"
    },
    {
      "src": "/api/v1/user/(.*)",
      "dest": "index.js"
    },
    {
      "src": "/api/v1/room/(.*)",
      "dest": "index.js"
    },
    
    {
      "src": "/api/v1/order/(.*)",
      "dest": "index.js"
    },
    {
      "src": "/api/v1/upload/(.*)",
      "dest": "index.js"
    },
    {
      "src": "/api/v1/reservations/(.*)",
      "dest": "index.js"
    },
    {
      "src": "/(.*)",
      "dest": "index.js"
    }
  ]
}
